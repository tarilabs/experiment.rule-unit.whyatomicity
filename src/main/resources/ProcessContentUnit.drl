package org.drools.experiment.rule_unit.whyatomicity;
unit ProcessContentUnit

rule PROCESS_CONTENT
when
    $b: Box( $content: content )
then
    // process content
    System.out.println("Inside the box "+$b+" found: "+$content);
end
